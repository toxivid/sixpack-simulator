local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "sigma", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

_G.autoclick = true
_G.autowinbattle = true
_G.autorebirth = true
_G.autobattle = true

function autoclicker()
    while _G.autoclick == true do
        game:GetService("ReplicatedStorage").packages._Index:FindFirstChild("sleitnick_knit@1.5.1").knit.Services.DumbellService.RF.OnClickRequest:InvokeServer()
        wait(0.00000000000000000000000000001)
    end
end

function autowinbattler()
    while _G.autowinbattle == true do
        game:GetService("ReplicatedStorage").packages._Index:FindFirstChild("sleitnick_knit@1.5.1").knit.Services.BattleService.RF.OnBattleClickRequest:InvokeServer()
        wait(0.00000000000000000000000000001)
    end
end

function autorebirther()
    while _G.autorebirth == true do
        game:GetService("ReplicatedStorage").packages._Index:FindFirstChild("sleitnick_knit@1.5.1").knit.Services.RebirthService.RF.OnRebirthRequest:InvokeServer()
        wait(0.00000000000000000000000000001)
    end
end

function autobattler()
    while _G.autobattle == true do
        local lastrebirths = game.Players.LocalPlayer.leaderstats.Rebirths.Value
        print(lastrebirths)
        wait(4.6)
        local VirtualInputManager = game:GetService("VirtualInputManager")
        VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.E, false, game)
        VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.E, false, game)
        if game.Players.LocalPlayer.leaderstats.Rebirths.Value == lastrebirths + 1 then
            print("player", game.Players.LocalPlayer, "rebirthed")
            print("sigma boy")
        end
    end
end

local Tab = Window:MakeTab({
	Name = "farm",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddToggle({
	Name = "autoclick",
	Default = false,
	Callback = function(Value)
        _G.autoclick = Value
        autoclicker()
    end
})

Tab:AddToggle({
	Name = "autowinbattle",
	Default = false,
	Callback = function(Value)
        _G.autowinbattle = Value
        autowinbattler()
    end
})

Tab:AddToggle({
	Name = "autorebirth",
	Default = false,
	Callback = function(Value)
        _G.autorebirth = Value
        autorebirther()
    end    
})

Tab:AddToggle({
	Name = "autobattle",
	Default = false,
	Callback = function(Value)
        _G.autobattle = Value
        OrionLib:MakeNotification({
        	Name = "debug",
	        Content = "make sure ur close to an enemy so u can battle it also if u want it to be afk also enable autowinbattle",
	        Image = "rbxassetid://4483345998",
	        Time = 5
            })
        autobattler()
    end    
})



